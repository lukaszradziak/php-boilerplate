{
  "name": "lukaszradziak/php-boilerplate",
  "description": "PHP boilerplate with composer and phpunit",
  "autoload": {
    "psr-4": {
      "App\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "require-dev": {
    "phpunit/phpunit": "^9"
  },
  "scripts": {
    "test": "vendor/bin/phpunit"
  }
}
